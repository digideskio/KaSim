digraph G{ 
node [shape=box, style=filled, fillcolor=lightskyblue];
"r0" ; 
"r1" ; 
"r2" ; 
"r3" ; 
"r4" ; 
"r5" ; 
"r6" ; 
"r7" ; 
edge [color=green, arrowhead=normal];
"r0" -> "r2"[label ="[0->0]"] ;
"r0" -> "r6"[label ="[0->0]"] ;
"r1" -> "r2"[label ="[0->1]"] ;
"r1" -> "r7"[label ="[0->0]"] ;
"r2" -> "r5"[label ="[0->0];[1->1]"] ;
"r3" -> "r4"[label ="[0->0];[1->1]"] ;
"r6" -> "r0"[label ="[0->0]"] ;
"r6" -> "r3"[label ="[0->0]"] ;
"r7" -> "r1"[label ="[0->0]"] ;
"r7" -> "r3"[label ="[0->1]"] ;

edge [color=red, arrowhead=tee];
"r0" -> "r3"[label ="[0->0]"] ; 
"r1" -> "r3"[label ="[0->1]"] ; 
"r3" -> "r5"[label ="[0->0];[1->1]"] ; 
"r5" -> "r3"[label ="[0->0];[1->1]"] ; 
