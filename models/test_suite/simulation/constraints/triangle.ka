%agent: A(b,c)
%agent: B(a,c)
%agent: C(a,b)

A(b), B(a) -> A(b!0), B(a!0) @1
B(c), C(b) -> B(c!0), C(b!0) @1
C(a), A(c) -> C(a!0), A(c!0) @1

%blacklist: A(b!1), B(a!1,c!2), C(b!2,a!3), A(c!3)
%blacklist: B(c!1), C(b!1,a!2), A(c!2,b!3), B(a!3)
%blacklist: C(a!1), A(c!1,b!2), B(a!2,c!3), C(b!3)

%init: 100 A(),B(),C()

%mod: [E] = [Emax] do $SNAPSHOT "final"